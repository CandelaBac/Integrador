{{#if producto.length}}
    <button class="btn__enviarCarrito">Enviar</button>
    <table>
        <tr>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Colores</th>
            <th>Foto</th>
            <th>Dimensiones</th>
            <th>Envio</th>
            <th>Acciones</th>
        </tr>
        <tr>
            <td>{{nombre}}</td>
            <td>{{precio}}</td>
            <td>{{colores}}</td>
            <td>
                <img src="{{imagen}}" alt="{{nombre}}">
            </td>
            <td>{{dimensiones}}</td>

            {{#if envio}}
                <td>Si</td>
            {{else}} 
                <td>No</td>
            {{/if}}       
            <td>
                <button>borrar</button>
            </td>
        </tr>
    </table>
{{else}}
    <h2>El carrito esta vacio</h2>    
{{/if}}    